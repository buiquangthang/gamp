<tr class="place" id="place_<%= place.id %>">
  <td><%= place_counter + 1 %></td>
  <td><%= place.code %></td>
  <td><%= place.address %></td>
  <td><%= place.title %></td>
  <td class="text-center">
    <%= check_box_tag "bus_route[list_places][]", place.id, false, class: "place-checkbox" %>
  </td>
</tr>

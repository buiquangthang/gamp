<div id="busstop-modal" class="modal fade course-member" role="dialog">
  <div class="modal-dialog">
    <div class="modal-content"> 
      <div id="hidden_project_id">
        <%= hidden_field_tag :bus_route_id, @bus_route.id %>
      </div>
      <div class="body">
        <div class="panel panel-success">
          <div class="search-user-form">
            <div class="search_users">
              <div class="form-group">
                <%= text_field_tag :bus_stop_search,
                params[:bus_stop_search],
                placeholder: t(".search"), class: "form-control" %> 
              </div>
            </div>
          </div>
          <div class="choose-all-member">
            <label>
              <%= check_box_tag "check", "check_all", false, class: "check_all" %>
              <%= t ".choose_all" %>
            </label>
          </div>
          <div class="add-user-table">           
            <table class="table table-striped table-hover table-bordered">
              <thead class="">
                <tr>
                  <th></th>
                </tr>
              </thead>
              <tbody id="list_find_users">
                <%= render "list_bus_stop", places: @another_places %>
              </tbody>
            </table> 
          </div>
        </div>
      </div>
      <div class="footer text-center create-member">
        <%= link_to t(".add"),"#", 
          class: "btn btn-primary defaut-btn", id: "btn-project-member",
          remote: true %> 
        <button type="button" class="btn btn-default" data-dismiss="modal">
          <%= t ".close" %>
        </button>
      </div>
    </div>
  </div>
</div>
